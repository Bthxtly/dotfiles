layout {
  // background-color "transparent"

  focus-ring {
    off
  }

  border {
    active-gradient from="#eb6f92" to="#c4a7e7" angle=-45
    inactive-color "#595959aa"
    urgent-color "#9b0000"
  }

  shadow {
    color "#00000070"
